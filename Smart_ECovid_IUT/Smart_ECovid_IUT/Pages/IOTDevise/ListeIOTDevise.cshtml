@page
@model Smart_ECovid_IUT.Pages.IOTDevise.ListeIOTDeviseModel
@using ClasseE_Covid.IOTDevise;
@{
}
<div>
    <div class="border-bottom mb-4">
        <h1>Liste IOT DEVISE</h1>

    </div>
    <div class="row">
        <div class="col-md-12 mb-4">
            <div class="card">
                <div class="card-header border-bottom row">
                    <div class="col-3">
                        <h3>Liste IOT DEVISE</h3>
                    </div>
                    <div class="col-9">
                        <input type="text" class="form-control label recher" id="recher" name="recher" placeholder="Rechercher nom box" />
                    </div>
                </div>

                <div id="ListIOT">
                    <partial name="PartialIOTDevise/_PartialListIOT" />
                </div>

            </div>
        </div>
    </div>
</div>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script>
    $(".recher").keyup(function (e) {

        var nomBox = $(this).val();

        $.ajax({
            type: "GET",
            url: "?handler=Recherche",
            data: {
                nomBox: nomBox,
            }
        }).done(function (result) {
            $("#ListIOT").html(result);
        })
    });

</script>