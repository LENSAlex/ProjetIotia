@page
@model Smart_ECovid_IUT.Pages.Utilisateur.ListeUtilisateurModel
@using ClasseE_Covid.Utilisateur
@{
    ViewData["Title"] = "Home page";
}
<div>
    <div class="border-bottom mb-4">

        <h1>Liste Utilisateur</h1>
    </div>
    <div class="row">
        <div class="col-md-12 mb-4">
            <div class="card">
                <div class="card-header border-bottom row">
                    <div class="col-3">
                        <h3>Liste Utilisateur</h3>
                    </div>
                    <div class="col-9">
                        <input type="text" class="form-control label recher" id="recher" name="recher" placeholder="Rechercher nom utilisateur" />
                    </div>
                </div>
                <div id="ListUser">
                    <partial name="PartialIOTDevise/_PatialListUtilisateur" />
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script>
    $(".recher").keyup(function (e) {

        var seach = $(this).val();

        $.ajax({
            type: "GET",
            url: "?handler=Recherche",
            data: {
                seach: seach,
            }
        }).done(function (result) {
            $("#ListUser").html(result);
        })
    });

</script>