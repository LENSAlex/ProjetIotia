@page
@model Smart_ECovid_IUT.Pages.Promotion.CréeModifPromotionModel
@{
    ViewData["Title"] = "Home page";
}
<div class="container">
    <div class="border-bottom mb-4">
        <h1> Création Promotion</h1>
    </div>
    <div class="row">
        <div class="col-md-12 mb-4">
            <div class="card mb-4">
                <div class="card-header border-bottom">
                    <h3>Création une Promotion</h3>
                </div>
                <div class="card-body border-bottom">
                    <form>
                        <div class="form-row">
                            <div class="col-md-4 mb-3">
                                <label for="Nom">Nom</label>
                                <input type="text" class="form-control" id="Nom" placeholder="Nom" required />
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="Nom">Année</label>
                                <input type="text" class="form-control" id="Ip" placeholder="Ip" required />
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="Nom">Durée</label>
                                <input type="text" class="form-control" id="Ip" placeholder="Ip" required />
                            </div>
                        </div>
                        <button class="btn btn-primary" type="submit">Submit form</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>