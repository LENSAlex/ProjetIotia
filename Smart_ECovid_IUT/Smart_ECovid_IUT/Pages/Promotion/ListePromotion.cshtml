@page
@model Smart_ECovid_IUT.Pages.Promotion.ListePromotionModel
@using ClasseE_Covid
@{

}
<div>
    <div class="border-bottom mb-4">
        <h1>Liste Promotion</h1>
    </div>
    <div class="row">
        <div class="col-md-12 mb-4">
            <div class="card">
                <div class="card-header border-bottom">
                    <h3>Liste Promotion</h3>
                </div>
                <div class=" table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th scope="col">Nom</th>
                                <th scope="col">Année</th>
                                <th scope="col">Durée</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (GitHubBranch gitHubBranch in Model.Branches)
                            {


                                <tr>
                                    <td>@gitHubBranch.Name</td>
                                    <td>@gitHubBranch.Id</td>
                                    <td>@gitHubBranch.Disponible</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>